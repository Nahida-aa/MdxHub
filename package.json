{
  "name": "mdxhub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:node": "ts-node --project tsconfig.node.json  scripts/watch-mdx.ts",
    "dev-w": "concurrently --kill-others \"next dev --turbopack\" \"pnpm watch:node\"",
    "dev": "pnpm gen && next dev --turbopack",
    "gen": "ts-node --project tsconfig.node.json scripts/build-mdx.ts",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prettier-fix": "",
    "test:watch": "ts-node --project tsconfig.node.json  tests/scripts/watch.ts",
    "export": "next export"
  },
